            {background.redux}
          </BackgroundText>
        </BackgroundTool>
      </div>
      <div class="swiper-slide">
        <BackgroundTool>
          <Vitejs />
          <BackgroundText>
            {background.vitejs}
          </BackgroundText>
        </BackgroundTool>
      </div>
      <div class="swiper-slide">
        <BackgroundTool>
          <Jest />
          <BackgroundText>
            {background.jest}
          </BackgroundText>
        </BackgroundTool>
      </div>
      <div class="swiper-slide">
        <BackgroundTool>
          <TestingLibrary />
          <BackgroundText>
            {background.testinglibrary}
