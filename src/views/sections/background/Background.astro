          </BackgroundText>
        </BackgroundTool>
      </div>
      <div class="swiper-slide">
        <BackgroundTool>
          <Git />
          <BackgroundText>
            {background.git}
          </BackgroundText>
        </BackgroundTool>
      </div>
      <div class="swiper-slide">
        <BackgroundTool>
          <Docker />
          <BackgroundText>
            {background.docker}
          </BackgroundText>
        </BackgroundTool>
      </div>
      <div class="swiper-slide">
        <BackgroundTool>
          <Azure />
          <BackgroundText>
            {background.azure}
          </BackgroundText>
        </BackgroundTool>
      </div>
    </div>
  </div>
</section>
